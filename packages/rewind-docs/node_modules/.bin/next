#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kuss/project/sides/rewind-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kuss/project/sides/rewind-docs/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/next@12.1.6_ef5jwxihqo6n7gxfmzogljlgcm/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/next@12.1.6_ef5jwxihqo6n7gxfmzogljlgcm/node_modules/next/dist/bin/next" "$@"
fi
