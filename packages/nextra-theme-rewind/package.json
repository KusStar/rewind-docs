{
  "name": "nextra-theme-rewind",
  "version": "0.0.1",
  "description": "A Nextra theme for Rewind app.",
  "main": "index.js",
  "module": "index.js",
  "repository": "https://github.com/kusstar/rewind-docs.git",
  "author": "KusStar",
  "license": "MIT",
  "files": [
    "dist/*",
    "style.css"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js"
    },
    "./callout": {
      "import": "./dist/callout.js"
    },
    "./bleed": {
      "import": "./dist/bleed.js"
    },
    "./tabs": {
      "import": "./dist/components/tabs.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\" \"pnpm types --watch\"",
    "build": "pnpm build:tailwind && pnpm build:layout && pnpm types",
    "build:layout": "node scripts/build.js",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsc -p tsconfig.types.json",
    "dev:layout": "node scripts/dev.js",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "rm -rf dist && pnpm build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@mdx-js/react": "^2.1.0",
    "@reach/skip-nav": "^0.16.0",
    "classnames": "^2.2.6",
    "flexsearch": "^0.7.21",
    "focus-visible": "^5.1.0",
    "framer-motion": "^6.3.11",
    "github-slugger": "^1.4.0",
    "intersection-observer": "^0.12.0",
    "match-sorter": "^4.2.0",
    "next-themes": "^0.2.0",
    "parse-git-url": "^1.0.1",
    "react-intersection-observer": "^9.3.3",
    "scroll-into-view-if-needed": "^2.2.29",
    "title": "^3.4.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.2",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.2",
    "cssnano": "^5.1.11",
    "nextra": "2.0.0-alpha.56",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.1.2"
  }
}
