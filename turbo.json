{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.js",
        "dist/**/*.css",
        ".next/**",
        "style.css",
        "index.js",
        "ssg.js",
        "loader.js"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}